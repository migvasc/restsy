    <!-- Header -->
    <header>
        <div class="container" id="maincontent" tabindex="-1">
            <div class="row">
                <div class="col-lg-12">
                    <img class="img-responsive" src="img/restsy_logo1.png"  height="300" width="300" alt="">
                    <div class="intro-text">
                        <h1 class="name">Restsy</h1>
                        <hr class="star-light">
                        <span class="skills">Refeições na hora certa!</span>
                        <%= form_tag(zomato_buscar_path, :method => "get", id: "search-form") do %>
                        <p> Para começar, Informe a cidade em que você está:</p>
                            <%= text_field_tag :search, params[:search], placeholder: "Search Recipes" %>
                            <%= submit_tag "Search", :name => nil %>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
    </header>

